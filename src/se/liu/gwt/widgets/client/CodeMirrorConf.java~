package se.liu.gwt.widgets.client;


public class CodeMirrorConf {
    String value;
    String mode;



    public String getValue(){
	return value;
    }
    public void setValue(String value){
	this.value = value;
    }
    public String getMode(){
	return mode;
    }
    public void setMode(String mode){
	this.mode = mode;
    }

}